(car.image)\n\t\t\t\t\tsetModel(car.model)\n\t\t\t\t\tsetBrand(car.brand)\n\t\t\t\t\tsetCategory(car.category)\n\t\t\t\t\tsetCountInStock(car.countInStock)\n\t\t\t\t\tsetDescription(car.description)\n\t\t\t\t} else {\n\t\t\t\t\tsetName('Sample name')\n\t\t\t\t\tsetPrice(0)\n\t\t\t\t\tsetImage('images/sample.jpg')\n\t\t\t\t\tsetModel('Sample model')\n\t\t\t\t\tsetBrand('Sample brand')\n\t\t\t\t\tsetCategory('Sample category')\n\t\t\t\t\tsetCountInStock(0)\n\t\t\t\t\tsetDescription('Sample description')\n\t\t\t\t}\n\t\t\t}\n\t\t\tconsole.log('wtf')\n\t\t},\n\t\t[ dispatch, history, carBrand, carModel, carId, successUpdate, car.name ]\n\t)\n\n\tconst uploadFileHandler = async (e) => {\n\t\tconst file = e.target.files[0]\n\t\tconst formData = new FormData()\n\t\tformData.append('image', file)\n\t\tsetUploading(true)\n\n\t\ttry {\n\t\t\tconst config = {\n\t\t\t\theader: {\n\t\t\t\t\t'Content-Type': 'multipart/form-data'\n\t\t\t\t}\n\t\t\t}\n\t\t\tconst { data } = await axios.post('/api/upload', formData, config)\n\n\t\t\tsetImage(data)\n\t\t\tsetUploading(false)\n\t\t} catch (error) {\n\t\t\tconsole.error(error)\n\t\t\tsetUploading(false)\n\t\t}\n\t}\n\n\tconst submitHandler = (e) => {\n\t\te.preventDefault()\n\t\tdispatch(\n\t\t\tupdateCar({\n\t\t\t\t_id: carId,\n\t\t\t\tname,\n\t\t\t\tprice,\n\t\t\t\timage,\n\t\t\t\tmodel,\n\t\t\t\tbrand,\n\t\t\t\tcategory,\n\t\t\t\tdescription,\n\t\t\t\tcountInStock\n\t\t\t})\n\t\t)\n\t}\n\n\treturn (\n\t\t<div>\n\t\t\t<Link to='/admin/carlist' className='btn btn-light my-3'>\n\t\t\t\tGo Back\n\t\t\t</Link>\n\t\t\t<FormContainer>\n\t\t\t\t{pathname.includes(carModel) ? <h1>Edit Car Information</h1> : <h1>Add New Car Information</h1>}\n\n\t\t\t\t{loadingUpdate && <Loader />}\n\t\t\t\t{errorUpdate && <Message variant='danger'>{errorUpdate}</Message>}\n\t\t\t\t{loading ? (\n\t\t\t\t\t<Loader />\n\t\t\t\t) : error ? (\n\t\t\t\t\t<Message variant='danger'>{error}</Message>\n\t\t\t\t) : (\n\t\t\t\t\t<Form onSubmit={submitHandler}>\n\t\t\t\t